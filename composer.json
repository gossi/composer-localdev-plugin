{
	"name" : "gossi/composer-localdev-plugin",
	"type" : "composer-plugin",
	"description" : "Composer Plugin for local development",
	"license" : "MIT",
	"keywords" : [
		"composer",
		"local",
		"development"
	],
	"autoload" : {
		"psr-4" : {
			"gossi\\composer\\localdev\\" : "src"
		}
	},
	"require" : {
		"php" : ">=5.3",
		"composer-plugin-api" : "^1.0"
	},
	"require-dev" : {
		"composer/composer" : "master-dev"
	},
	"extra" : {
		"class" : "\\gossi\\composer\\localdev\\ComposerLocaldevPlugin"
	}
}